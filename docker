# run docker myslq
docker run --name guru-myslq3 -e MYSQL_ALLOW_EMPTY_PASSWORD=yes -v /home/gvr/tmp:/var/lib/mysql -p 3306:3306 -d mysql

# start cent os and run infinite loop on it
docker run -d centos tail -f /dev/null

# see running docker containers
docker ps

# login to docker bash
docker exec -it compassionate_robinson bash

# exit docker container bash

